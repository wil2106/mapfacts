{
  "expo": {
    "name": "MapFacts",
    "slug": "mapfacts",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "associatedDomains": ["applinks:mapfacts.vercel.app"],
      "supportsTablet": true,
      "bundleIdentifier": "com.wil2106.mapfacts",
      "config": {
        "googleMapsApiKey": "AIzaSyC09JZcwYyzqeLJo2BxGN3HtD_dJd2CEMw"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ec8ef3cc-c14e-494d-a9c0-73319ddaeebc"
      },
      "googlePlacesApiKey": "AIzaSyBA9ITaUEFWML5Of4qCkBvEPYtGIxIx91M"
    }
  }
}
